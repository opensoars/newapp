{
  "name": "es7",
  "description": "EcmaScript 7 boilerplate project",
  "version": "0.0.1",
  "author": "opensoars",

  "scripts": {
    "test": "node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha \"./test/mocha/*.js\" && npm run coveralls",
    "local_test": "node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha \"./test/mocha/*.js\"",
    "coveralls": "node node_modules/coveralls/bin/coveralls.js < coverage/lcov.info",
    "doc": "node node_modules/jsdoc/jsdoc.js -c ./doc/config.json --readme README.md",

    "gulp-babel": "node node_modules/gulp/bin/gulp.js babel",
    "gulp-watch": "node node_modules/gulp//bin/gulp.js watch"
  },

  "engines": {
    "node": ">=5.1.0"
  },

  "dependencies": {
    
  },

  "devDependencies": {
    "babel": "^6.3.13",
    "babel-plugin-syntax-async-functions": "^6.3.13",
    "babel-plugin-transform-async-to-generator": "^6.3.13",
    "babel-plugin-transform-es2015-destructuring": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "coveralls": "^2.11.4",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-watch": "^4.3.5",
    "istanbul": "^0.4.0",
    "jsdoc": "^3.3.3",
    "mocha": "^2.3.4"
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/opensoars/es7"
  },

  "keywords": [
    "es7",
    "EcmaScript",
    "JavaScript",
    "transpile",
    "boilerplate"
  ],

  "readmeFilename": "README.md",

  "license": "MIT"
}